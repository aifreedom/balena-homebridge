  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/rhwood/balena-homebridge/issues"
  },
  "homepage": "https://github.com/rhwood/balena-homebridge#readme",
  "dependencies": {
    "homebridge": "1.1.1",
    "homebridge-config-ui-x": "4.23.2",
    "homebridge-lutron-caseta-smockle": "3.0.36"
  }
}
